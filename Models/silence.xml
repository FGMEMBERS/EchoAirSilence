<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>silence.ac</path>

  <nasal>
    <load>
      var livery_update = aircraft.livery_update.new("Aircraft/Sky-sportster/Models/Liveries", 10);

      var self = cmdarg();

      var float_0 = self.getNode("sim/multiplay/generic/float[0]", 1);
      var float_1 = self.getNode("sim/multiplay/generic/float[1]", 1);
      
      var crew_door = self.getNode("sim/model/door-positions/crew/position-norm", 1);
      var propeller_door = self.getNode("sim/model/door-positions/propeller/position-norm", 1);
      
      crew_door.alias(float_0);
      propeller_door.alias(float_1);
    </load>

    <unload>
      livery_update.stop();
      crew_door.unalias();
      propeller_door.unalias();
    </unload>
  </nasal>

